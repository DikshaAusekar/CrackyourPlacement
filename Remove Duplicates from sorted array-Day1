class Solution {
    public int removeDuplicates(int[] nums) {
        if (nums.length == 0) return 0; // Handle edge case for empty array

        int i = 1, j = 1;
        int n = nums.length;

        while (j < n) {
            if (nums[j] > nums[i - 1]) {
                nums[i] = nums[j];
                i++;
            }
            j++;
        }
        return i;
    }
}
